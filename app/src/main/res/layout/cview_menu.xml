<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/group_menu"
    android:layout_width="match_parent"
    android:layout_height="70dp"
    android:background="@drawable/layout_value">


    <ImageView
        android:id="@+id/menu_home"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginLeft="30dp"
        android:src="@drawable/ic_home"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/menu_write"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginLeft="45dp"
        android:src="@drawable/ic_add_photo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@id/menu_home"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/menu_list"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginLeft="45dp"
        android:src="@drawable/ic_list"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@id/menu_write"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/menu_setting"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginLeft="45dp"
        android:src="@drawable/ic_setting"
        app:layout_constraintLeft_toRightOf="@+id/menu_list"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <!--<com.ismaeldivita.chipnavigation.ChipNavigationBar-->
    <!--android:id="@+id/bottom_menu"-->
    <!--android:layout_width="wrap_content"-->
    <!--android:layout_height="match_parent"-->
    <!--app:cnb_orientationMode="vertical" />-->


</androidx.constraintlayout.widget.ConstraintLayout>